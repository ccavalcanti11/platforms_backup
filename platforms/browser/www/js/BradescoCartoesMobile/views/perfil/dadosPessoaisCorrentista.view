<div id="header-hide-arrow" data-role="header" data-position="fixed" data-tap-toggle="false">
	<a id="btnLeftPanel" href="#perfilEditar" class="btn-back"></a>
	<h1>Dados de contato</h1>
</div>

<% 
	var user = AWBE.sessionStorage.getItem('user');
	if(AWBE.localStorage.getItem('isNCLegado_' + user.cpf) == "true"){ %>
	<div data-awbe-component="awbe-include" data-awbe-component-params="menuOpcoesLogadoNCLegado"></div>
<% }else{ %>
	<div data-awbe-component="awbe-include" data-awbe-component-params="menuOpcoesLogado"></div>
<% }%>

<div class="ui-content dadosPessoais" data-role="content">
	<form action="#dispositivoSegurancaEditarDadosPessoais">
	<input type="hidden" data-awbe-bind="cpf" id="cpf"/>
		
		<div class="divAlertas">
			<hr class="hr-margin-error">
			<table class="ui-table-generico-semBorder">
				<tr>
					<th class="col-12 icon-20-table"></th>
					<th class="col-88 fnt-book color-gray-lighter txt-08em" id="mensagemDivAlertas"></th>
				</tr>
			</table>
			<hr class="hr-margin-error">
		</div>
		
		<p class="fnt-book txt-1em color-gray-darker">E-mail</p>
		<input class="inputPlaceholder" type="email" placeholder="cliente@email.com.br" name="confirma_dado" id="email" data-awbe-bind="email_cadastro" data-awbe-for="dadosContatoValidation" value="<%=user.emailCadastro%>"/>
		<div class="ui-grid-b">
			<p class="fnt-book txt-1em color-gray-darker">Celular</p>
			<div class="ui-block-a width-31em">
				<input class="inputPlaceholder" type="tel" name="ddd_celular" data-awbe-for="*" id="ddd-celular" data-awbe-bind="ddd_celular" maxlength="2" pattern="[0-9]*" data-awbe-component="ddd" value=<%=user.dddCelular%>/>
			</div>
			<div class="ui-block-b margin-left-16">
				<div class="ui-block-c width-93em">
					<input class="inputPlaceholder" data-awbe-component="telefone" type="tel" name="numero_celular" id="numero-celular" data-awbe-bind="numero_celular" data-awbe-for="dadosPessoaisValidation" maxlength="11" value=<%=user.numeroCelular%>/>
				</div>
			</div>
		</div>
	</form>
	<div class="hr-margin-tb"></div>
	<div class="ui-grid-a">
		<div class="ui-block-a disabledButton tira-disabled" style="float:right" >
			<div class="ui-btn-arrow-blue-arrow"></div>
			<div class="ui-btn-arrow-blue" style="float:right">
				<a id="botaoSubmitDadosPessoais" name="botaoSubmitDadosPessoais">Continuar</a>
		    </div>
		</div>
	   <div class="ui-block-b"></div>
	</div>
</div>

<div data-awbe-component="popup"
	data-awbe-component-option-theme="a"
	data-awbe-component-option-theme-modal="b"
	data-awbe-component-option-modal="true"
	data-awbe-component-popup-id="dispositivoInexistente">
	<p class="titulo-modal">Dispositivo Inexistente</p>
	<p class="texto-modal-normal">Ser&aacute; necess&aacute;rio atualizar seu dispositivo de seguran&ccedil;a para acessar esse aplicativo.</p>
	<p class="texto-modal-normal">Por favor, entre em contato com sua ag&ecirc;ncia ou Central de Relacionamento.</p>
	<div class="align-botoes">
		<a id="dispositivoInexistenteClose" data-rel="back" class="botao-modal-002">Fechar</a>
	</div>
</div>

<div data-awbe-component="popup"
	data-awbe-component-option-theme="a"
	data-awbe-component-option-theme-modal="b"
	data-awbe-component-option-modal="true"
	data-awbe-component-popup-id="alertaEmail">
	<p  class="titulo-modal">E-mail Incompleto</p>
	<p class="texto-modal-normal">O endere&ccedil;o de e-mail precisa ter um &ldquo;nome&rdquo;, um &ldquo;@&rdquo; e um complemento, por exemplo <span>&ldquo;cliente@email.com.br&rdquo;</span>.</p>
	<div class="align-botoes">
		<a href="#" data-rel="back" class="botao-modal-002">Fechar</a>
	</div>
</div>

<div data-awbe-component="popup"
    data-awbe-component-option-theme="a"
    data-awbe-component-option-theme-modal="b"
    data-awbe-component-option-modal="true"
    data-awbe-component-popup-id="alertaTelefone">
	<p  class="titulo-modal"></p>
	<p class="texto-modal-normal">Erro ao cadastrar o celular.</p>
	<p class="texto-modal-normal">Tente novamente.</p>
	<div class="align-botoes">
		<a href="#" data-rel="back" class="botao-modal-002">OK</a>
    </div>
</div>

<div data-awbe-component="popup" 
	data-awbe-component-option-theme="a"
	data-awbe-component-option-theme-modal="b"
	data-awbe-component-option-modal="true"
	data-awbe-component-popup-id="popup-generico">
	<p class="titulo-modal" id="titulo-modal-personalizado"></p>
	<p class="texto-modal-normal" id="mensagem-personalizada"></p>
 		<div class="align-botoes">	
 			<a href="#" data-rel="back"  class="botao-modal-002">FECHAR</a>
 		</div>
</div>	

<div data-awbe-component="popup" 
	data-awbe-component-option-theme="a"
	data-awbe-component-option-theme-modal="b"
	data-awbe-component-option-modal="true"
	data-awbe-component-popup-id="dispositivoBloqueado">
	<p class="titulo-modal">Chave de Seguran√ßa desativada</p>
	<p class="texto-modal-normal">Para reativ&aacute;-la procure sua ag&ecirc;ncia ou contate o Fone F&aacute;cil Bradesco para outras informa&ccedil;&otilde;es.</p>
	<div class="align-botoes">	
		<a href="#meusCartoes" data-rel="back" class="botao-modal-002">FECHAR</a>
	</div>
</div>

<div data-awbe-component="popup"
	data-awbe-component-option-theme="a"
	data-awbe-component-option-theme-modal="b"
	data-awbe-component-option-modal="true"
	data-awbe-component-popup-id="popupEmailEmUso">
	<p class="titulo-modal">E-mail em uso</p>
	<p class="texto-modal-normal">O e-mail informado j&aacute; est&aacute; sendo utilizado em outro perfil.</p>
	<p class="texto-modal-normal">Por favor, utilize outro.</p>
	<div class="align-botoes">
		<a href="#" data-rel="back" class="botao-modal-002">Fechar</a>
	</div>
</div>

